  },
  "require-dev": {
    "phpunit/phpunit": "8.4.*",
    "fusionspim/php-cs-fixer-config": "^3.0"
  },
  "autoload": {
    "psr-4": {"Fusions\\PhpMarkdownToc\\":"src/"}
  }
}
